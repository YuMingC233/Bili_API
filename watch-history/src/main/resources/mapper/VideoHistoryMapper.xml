<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.dreamwolf.watchhistory.dao.VideoHistoryDao">
    <resultMap id="BaseResultMap" type="com.dreamwolf.watchhistory.entities.VideoHistory">
        <id column="uID" property="uID" javaType="INT"/>
        <id column="bvID" property="bvID" javaType="INT"/>
        <id column="CloseTime" property="CloseTime" javaType="DATE"/>
        <id column="TimelinePosition" property="TimelinePosition" javaType="DATE"/>
    </resultMap>
    <select id="getCxqb" parameterType="int" resultMap="BaseResultMap">
        SELECT * FROM videohistory WHERE bvID=#{id}
    </select>
</mapper>